
#include<iostream>
using namespace std;
int main()
{
    int size,max=0,max1=0;
    cin>>size;
    int arr[size];
        for(int index=0;index<size;++index)
            cin>>arr[index];
        for(int index=0;index<size;++index)
        {
            if(max<arr[index])
            max=arr[index];
        }
        for(int index=0;index<size;++index)
        {
            if(max1<arr[index] && arr[index]!=max)
            max1=arr[index];
        }    
    cout<<max1;
    return 0;    
}
